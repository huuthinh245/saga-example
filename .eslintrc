{
  "extends": ["airbnb", "prettier"],
  // "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [true, { "extensions": [".js", ".jsx"] }],
    "react/require-default-props": [true],
    "react/prop-types": [true],
    "import/prefer-default-export": true,
    "no-underscore-dangle": [true],
    "react/prefer-stateless-function": [true]
  }
}
